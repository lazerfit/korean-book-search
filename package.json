{
	"name": "korean-book-search-plugin",
	"version": "1.4.1",
	"description": "bookSearch is a plugin for Obsidian that allows you to search for books in your library and view their details.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/eslint-plugin-jsx-a11y": "^6.10.0",
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.31.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.5.3",
		"globals": "^16.3.0",
		"jiti": "^2.5.1",
		"prettier": "^3.6.2",
		"process": "^0.11.10",
		"typescript-eslint": "^8.37.0"
	}
